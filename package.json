{
	"name": "express-ts-react",
	"version": "0.1.0-alpha",
	"main": "index.js",
	"author": "hund snc",
	"description": "Express /w SSR Enabled React + Typescript starter",
	"keywords": [],
	"license": "ISC",
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1",
		"watch:client": "webpack --watch --mode=development --env target=client --config frontend/webpack.config.js",
		"watch:server": "webpack --watch --mode=development --env target=server --config webpack.config.js",
		"watch": "rimraf bundle && concurrently \"npm run watch:client\" \"npm run watch:server\"",
		"serve": "nodemon bundle/index.js",
		"bundle:client": "webpack --mode=production --config frontend/webpack.config.js",
		"bundle:server": "webpack --mode=production",
		"build": "rimraf bundle && npm run bundle:client && npm run bundle:server"
	},
	"devDependencies": {
		"@svgr/webpack": "^6.5.0",
		"@types/cors": "^2.8.12",
		"@types/express": "^4.17.14",
		"@types/http-errors": "^1.8.2",
		"@types/node": "^18.11.0",
		"@types/react": "^18.0.21",
		"@types/react-dom": "^18.0.6",
		"concurrently": "^7.4.0",
		"css-loader": "^6.7.1",
		"filemanager-webpack-plugin": "^7.0.0",
		"mini-css-extract-plugin": "^2.6.1",
		"sass": "^1.55.0",
		"sass-loader": "^13.1.0",
		"style-loader": "^3.3.1",
		"ts-loader": "^9.4.1",
		"tsconfig-paths-webpack-plugin": "^4.0.0",
		"typescript": "^4.8.4",
		"webpack": "^5.74.0",
		"webpack-cli": "^4.10.0",
		"webpack-merge": "^5.8.0",
		"webpack-node-externals": "^3.0.0"
	},
	"dependencies": {
		"body-parser": "^1.20.1",
		"cors": "^2.8.5",
		"dotenv": "^16.0.3",
		"ejs": "^3.1.8",
		"express": "^4.18.2",
		"http-errors": "^2.0.0",
		"react": "^18.2.0",
		"react-dom": "^18.2.0"
	}
}
